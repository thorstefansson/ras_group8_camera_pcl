    <remap from="input" to="/camera/depth/points"/>
<remap from="output" to="/outlier_removed_points"/>
  </node>




<!--Start voxel node -->
<node pkg="ras_group8_camera_pcl" type="example_voxelgrid" name="ras_group8_voxelgrid">

    <remap from="input" to="/outlier_removed_points"/>
<remap from="output" to="/voxel_points"/>
  </node>



<!--Start planar segmentation node -->
<node pkg="ras_group8_camera_pcl" type="example_planarsegmentation" name="ras_group8_planarsegmentation" output="screen">
